{
    "extends": "recommended",
    "rules": {
        "CatchException": {
            "enabled": true
        },
        "CatchThrowable": {
            "enabled": false
        },
        "ClassJavadoc": {
            "enabled": true
        },
        "ClosureAsLastMethodParameter": {
            "enabled": true
        },
        "DuplicateNumberLiteral": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "FieldTypeRequired": {
            "enabled": false
        },
        "JavaIoPackageAccess": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "MethodSize": {
            "enabled": false
        },
        "NoDef": {
            "enabled": true
        },
        "PrintStackTrace": {
            "enabled": false
        },
        "PropertyName": {
            "enabled": false
        },
        "SpaceAroundMapEntryColon": {
            "enabled": true
        },
        "SystemExit": {
            "enabled": false
        },
        "UnnecessaryGetter": {
            "enabled": false
        },
        "UnnecessaryObjectReferences": {
            "enabled": false
        },
        "UnnecessarySetter": {
            "enabled": false
        },
        "VariableName": {
            "enabled": true
        },
        "VariableTypeRequired": {
            "enabled": true
        }
    }
}
